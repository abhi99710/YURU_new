<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#E0EEFA"
    android:layout_height="match_parent"
    tools:context=".ui.transition.SleepEnhancer2">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="15dp"
        android:background="#E0EEFA"
        android:orientation="horizontal"
        app:cardCornerRadius="@dimen/_5sdp"
        app:cardElevation="@dimen/_5sdp"
        tools:context=".ui.dashboard.Sleep_enhancer_two">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="5dp">

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                android:text="@string/sleep_enhancer"
                android:textColor="@color/color_text"
                android:textSize="24sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/save_sleep_enhancer_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="16dp"
                android:text="@string/save"
                android:textColor="@color/colorPrimary"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_se2"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:background="@drawable/corner_videoview"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintVertical_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView11">

                <ImageView
                    android:id="@+id/o_option"
                    android:layout_width="@dimen/_50sdp"
                    android:layout_height="@dimen/_50sdp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/setting_o"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/e_option"
                    android:layout_width="@dimen/_50sdp"
                    android:layout_height="@dimen/_50sdp"
                    android:background="@drawable/setting_c"
                    app:layout_constraintEnd_toStartOf="@+id/c_option"
                    app:layout_constraintStart_toEndOf="@+id/o_option"
                    app:layout_constraintTop_toTopOf="@+id/o_option" />

                <ImageView
                    android:id="@+id/c_option"
                    android:layout_width="@dimen/_50sdp"
                    android:layout_height="@dimen/_50sdp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:background="@drawable/setting_e"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/a1Option"
                    android:layout_width="@dimen/_50sdp"
                    android:layout_height="@dimen/_50sdp"
                    android:background="@drawable/setting_a"
                    app:layout_constraintEnd_toStartOf="@+id/e_option"
                    app:layout_constraintStart_toEndOf="@+id/o_option"
                    app:layout_constraintTop_toBottomOf="@+id/o_option" />

                <ImageView
                    android:id="@+id/n1option"
                    android:layout_width="@dimen/_50sdp"
                    android:layout_height="@dimen/_50sdp"
                    android:background="@drawable/setting_n"
                    app:layout_constraintEnd_toStartOf="@+id/c_option"
                    app:layout_constraintStart_toEndOf="@+id/e_option"
                    app:layout_constraintTop_toBottomOf="@+id/c_option" />

                <ImageView
                    android:id="@+id/sl2_vector1"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_200sdp"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/vector1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/a1Option" />

                <ImageView
                    android:id="@+id/bottom_1_sl2"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_marginStart="60dp"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/sysbasricon"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/sl2_vector1" />

                <ImageView
                    android:id="@+id/arrowLeft1_sl2"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:background="@drawable/ic_baseline_keyboard_arrow_left_24"
                    app:layout_constraintEnd_toStartOf="@+id/bottom_1_sl2"
                    app:layout_constraintTop_toBottomOf="@+id/bottom_1_sl2" />

                <ImageView
                    android:id="@+id/center1_sl2"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:background="@drawable/ic_baseline_stop_24"
                    app:layout_constraintEnd_toStartOf="@+id/arrowRight1_sl2"
                    app:layout_constraintStart_toEndOf="@+id/arrowLeft1_sl2"
                    app:layout_constraintTop_toBottomOf="@+id/bottom_1_sl2" />

                <ImageView
                    android:id="@+id/center2_sl2"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:background="@drawable/ic_baseline_stop_24"
                    app:layout_constraintEnd_toStartOf="@+id/arrowRight2_sl2"
                    app:layout_constraintStart_toEndOf="@+id/arrowLeft2_sl2"
                    app:layout_constraintTop_toBottomOf="@+id/bottom2_sl2" />

                <ImageView
                    android:id="@+id/arrowLeft2_sl2"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:background="@drawable/ic_baseline_keyboard_arrow_left_24"
                    app:layout_constraintEnd_toStartOf="@+id/bottom2_sl2"
                    app:layout_constraintTop_toBottomOf="@+id/bottom2_sl2" />

                <ImageView
                    android:id="@+id/bottom2_sl2"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="64dp"
                    android:background="@drawable/sysbasricon"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/sl2_vector1" />

                <ImageView
                    android:id="@+id/arrowRight1_sl2"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:background="@drawable/ic_baseline_keyboard_arrow_right_24"
                    app:layout_constraintStart_toEndOf="@+id/bottom_1_sl2"
                    app:layout_constraintTop_toBottomOf="@+id/bottom_1_sl2" />

                <ImageView
                    android:id="@+id/arrowRight2_sl2"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:background="@drawable/ic_baseline_keyboard_arrow_right_24"
                    app:layout_constraintStart_toEndOf="@+id/bottom2_sl2"
                    app:layout_constraintTop_toBottomOf="@+id/bottom2_sl2" />

                <com.app.yuru.ui.transition.VerticalSeekBar
                    android:id="@+id/seekBar2"
                    android:layout_width="11dp"
                    android:layout_height="200dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginBottom="8dp"
                    android:background="#fff"
                    android:scrollIndicators="top"
                    android:scrollbarSize="@dimen/_20sdp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>